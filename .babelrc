{
  "presets": [ "react", [ "es2015", { "loose": true } ], "stage-0" ],
  "plugins": [
    [ "extensible-destructuring", { "mode": "optout", "impl": "immutable" } ],
    "lodash",
    "add-module-exports",
    "transform-decorators-legacy"
  ],
  "env": {
    "production": {
      "plugins": [ "transform-react-remove-prop-types" ]
    }
  }
}
